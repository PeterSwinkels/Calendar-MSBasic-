10 DEFINT A-Z
20 DAYNAME$(0) = "sa"
30 DAYNAME$(1) = "su"
40 DAYNAME$(2) = "mo"
50 DAYNAME$(3) = "tu"
60 DAYNAME$(4) = "we"
70 DAYNAME$(5) = "th"
80 DAYNAME$(6) = "fr"
90 DEF FNLEAP (YEAR) = ((YEAR MOD 400 = 0) OR ((YEAR MOD 4 = 0) AND (NOT (YEAR MOD 100 = 0))))
100 DEF FNWEEKDAY (YEAR, MONTH, DAY) = ((DAY + INT((13 * (MONTH + 1)) / 5) + YEAR + INT(YEAR / 4) - INT(YEAR / 100) + INT(YEAR / 400)) MOD 7)
110 MONTH = VAL(LEFT$(DATE$, 2))
120 DAY = VAL(MID$(DATE$, 4, 2))
130 YEAR = VAL(RIGHT$(DATE$, 4))
140 GOSUB 260
150 PRINT MONTHNAME$; YEAR
160 GOSUB 180
170 END
180 FOR INDEX = 0 TO 6
190 PRINT " "; DAYNAME$(FNWEEKDAY(YEAR, MONTH, INDEX + 1)); "  ";
200 NEXT INDEX
210 PRINT
220 FOR INDEX = 1 TO DAYCOUNT
230 IF INDEX = DAY THEN PRINT USING "[##] "; INDEX;  ELSE PRINT USING " ##  "; INDEX;
240 IF INDEX MOD 7 = 0 THEN PRINT
250 NEXT INDEX
260 RESTORE 330
270 FOR INDEX = 1 TO MONTH
280 READ DAYCOUNT, MONTHNAME$
290 IF INDEX = MONTH THEN RETURN
300 NEXT INDEX
310 IF MONTH = 2 AND FNLEAP(YEAR) THEN DAYCOUNT = DAYCOUNT + 1
320 RETURN
330 DATA 31, "January"
340 DATA 28, "February"
350 DATA 31, "March"
360 DATA 30, "April"
370 DATA 31, "May"
380 DATA 30, "June"
390 DATA 31, "July"
400 DATA 31, "August"
410 DATA 30, "September"
420 DATA 31, "October"
430 DATA 30, "November"
440 DATA 31, "December"
